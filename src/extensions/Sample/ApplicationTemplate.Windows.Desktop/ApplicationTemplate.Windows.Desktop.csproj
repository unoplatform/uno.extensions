<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows10.0.19041.0</TargetFramework>
    <TargetPlatformMinVersion>10.0.17763.0</TargetPlatformMinVersion>
    <RootNamespace>ApplicationTemplate</RootNamespace>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Platforms>x86;x64</Platforms>
    <RuntimeIdentifiers>win10-x86;win10-x64</RuntimeIdentifiers>
    <LangVersion>9.0</LangVersion>
    <DefineConstants>WINDOWS,WINUI</DefineConstants>
    <DefaultLanguage>en</DefaultLanguage>
    <UseWinUI>true</UseWinUI>
  </PropertyGroup>

  <ItemGroup>
    <Manifest Include="$(ApplicationManifest)" />

    <PackageReference Include="System.Reactive" Version="5.0.0" />
    <PackageReference Include="System.IO.FileSystem" Version="4.3.0" />
    <PackageReference Include="System.IO.FileSystem.Primitives" Version="4.3.0" />
    <PackageReference Include="System.Reflection.Emit.Lightweight" Version="4.7.0" />

    <PackageReference Include="Microsoft.ProjectReunion" Version="0.8.0"/>

    <PackageReference Include="CommunityToolkit.Mvvm" Version="7.0.2" />
    <PackageReference Include="FluentValidation.DependencyInjectionExtensions" Version="9.3.0" />
    <PackageReference Include="Microsoft.Extensions.Localization.Abstractions" Version="5.0.7" />
    <PackageReference Include="Serilog.Extensions.Hosting" Version="4.1.2" />
    <PackageReference Include="Serilog.Settings.Configuration" Version="3.1.0" />
    <PackageReference Include="Serilog.Sinks.Debug" Version="2.0.0" />
    <!--<PackageReference Include="Uno.CodeGen" Version="1.34.0-dev.175" />
    <PackageReference Include="Uno.Injectable" Version="1.34.0-dev.175" />
    <PackageReference Include="Uno.SourceGenerationTasks" Version="3.1.0"/>-->
    <PackageReference Include="Xamarin.Essentials.Interfaces" Version="1.5.3.2" />
    <PackageReference Include="Refit" Version="6.0.38" />
    <PackageReference Include="Reactive.Annex" Version="0.3.0-dev.18" />
    <PackageReference Include="nventive.Nimue.Application.Building.Light">
      <Version>0.1.0-alpha.73</Version>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="nventive.Nimue.Application.Packaging">
      <Version>0.1.0-alpha.73</Version>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="nventive.Nimue.Application.Versioning">
      <Version>0.1.0-alpha.73</Version>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\uno.extensions.Hosting\Uno.Extensions.Hosting.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Http.Firebase\Uno.Extensions.Http.Firebase.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Http\Uno.Extensions.Http.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Http.Refit\Uno.Extensions.Http.Refit.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Configuration\Uno.Extensions.Configuration.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Localization\Uno.Extensions.Localization.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Logging\Uno.Extensions.Logging.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Logging.Serilog\Uno.Extensions.Logging.Serilog.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Navigation\Uno.Extensions.Navigation.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Serialization\Uno.Extensions.Serialization.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Serialization.Http\Uno.Extensions.Serialization.Http.csproj" />
    <ProjectReference Include="..\..\uno.extensions.Serialization.Refit\Uno.Extensions.Serialization.Refit.csproj" />
  </ItemGroup>

  <Import Project="..\ApplicationTemplate.Shared\ApplicationTemplate.Shared.projitems" Label="Shared" />
</Project>
