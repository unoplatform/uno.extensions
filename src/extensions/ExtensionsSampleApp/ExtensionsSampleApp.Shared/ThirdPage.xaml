<Page
    x:Class="ExtensionsSampleApp.ThirdPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ExtensionsSampleApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <TextBlock Text="Page 3" Margin="20" FontSize="30" />
            <Button Content="Next Page" Click="NextPageClick"/>
            <Button Content="Go to Fourth Page and remove third page - NavigationRequest" Click="GoToFourthPageRemoveOnePageNavigationRequestClick"/>
            <Button Content="Go to Fourth Page and remove second and third page - NavigationRequest" Click="GoToFourthPageRemoveTwoPagesNavigationRequestClick"/>
            <Button Content="Go Back - NavigateToPreviousView" Click="GoBackNavigateToPreviousViewClick"/>
        </StackPanel>
    </Grid>
</Page>
