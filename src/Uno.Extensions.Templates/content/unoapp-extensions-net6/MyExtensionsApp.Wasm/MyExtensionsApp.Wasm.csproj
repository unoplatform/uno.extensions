    <PackageReference Include="Uno.Toolkit.WinUI.Material" Version="1.2.1" />
    <PackageReference Include="Uno.Toolkit.WinUI" Version="1.2.1" />
    <PackageReference Include="Uno.SourceGenerationTasks" Version="4.0.2" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Uno.Extensions.Configuration" Version="255.255.255.255" />
