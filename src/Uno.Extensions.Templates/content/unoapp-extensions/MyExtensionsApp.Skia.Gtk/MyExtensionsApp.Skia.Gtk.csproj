<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType Condition="'$(Configuration)'=='Release'">WinExe</OutputType>
    <OutputType Condition="'$(Configuration)'=='Debug'">Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
		<DefineConstants>$(DefinedConstants);WINUI</DefineConstants>
	</PropertyGroup>
  <ItemGroup Condition="exists('..\MyExtensionsApp.UWP')">
    <EmbeddedResource Include="..\MyExtensionsApp.UWP\Package.appxmanifest" LogicalName="Package.appxmanifest" />
    <Content Include="..\MyExtensionsApp.UWP\Assets\StoreLogo.png" Link="Assets\StoreLogo.png" />
    <Content Include="Assets\Fonts\uno-fluentui-assets.ttf" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Include="..\MyExtensionsApp.Shared\**\*.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="6.0.0" />
    <PackageReference Include="Uno.WinUI.Skia.Gtk" Version="4.0.0-dev.5688" />
    <PackageReference Include="Uno.WinUI.RemoteControl" Version="4.0.0-dev.5690" Condition="'$(Configuration)'=='Debug'" />
    <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.0.0-dev.5726" />
		<PackageReference Include="Uno.Toolkit.WinUI" Version="0.1.0-dev.169" />
		<PackageReference Include="Uno.Toolkit.WinUI.Material" Version="0.1.0-dev.169" />
		<PackageReference Include="Uno.Core" Version="4.0.1" />
		<PackageReference Include="Uno.Material.WinUI" Version="1.1.0-dev.64" />
		<PackageReference Include="Uno.SourceGenerationTasks" Version="4.0.0" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Uno.Extensions.Configuration" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Core" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Hosting.WinUI" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Http" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Localization.WinUI" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Logging.Serilog" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Logging.WinUI" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Navigation.Toolkit.WinUI" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Navigation.Toolkit.WinUI" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Serialization" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Reactive" Version="0.1.0-PullRequest0166.754" />
		<PackageReference Include="Uno.Extensions.Reactive.WinUI" Version="0.1.0-PullRequest0166.754" />
	</ItemGroup>
	<Import Project="..\MyExtensionsApp.Shared\MyExtensionsApp.Shared.projitems" Label="Shared" />
</Project>
